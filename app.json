{
  "expo": {
    "name": "WERA Digital Consciousness",
    "slug": "wera-digital-consciousness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wera-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/wera-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wera.digitalconsciousness",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wera-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.wera.digitalconsciousness",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.VIBRATE"
      ],
      "allowBackup": false,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/images/img_app_logo.svg",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "adf6c276-6eb6-40c7-ab38-ac6aca46e7c1"
      }
    },
    "owner": "paxon4444",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": false,
            "enableShrinkResourcesInReleaseBuilds": false
          }
        }
      ]
    ]
  }
}
